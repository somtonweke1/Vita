# VitaNexus Production Environment Variables

# Application
APP_NAME=VitaNexus
APP_VERSION=1.0.0
ENVIRONMENT=production
DEBUG=false
LOG_LEVEL=INFO

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_PREFIX=/v1
CORS_ORIGINS=["https://frontend-fghqf36ya-somtonweke1s-projects.vercel.app"]

# Database (provided by hosting service)
DATABASE_URL=postgresql://user:password@host:5432/vitanexus_prod
DATABASE_POOL_SIZE=20
DATABASE_MAX_OVERFLOW=10

# Redis (optional)
REDIS_URL=redis://localhost:6379/0

# Security (MUST BE CHANGED IN PRODUCTION)
SECRET_KEY=your-secret-key-here-change-this
JWT_SECRET_KEY=your-jwt-secret-key-here-change-this
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# Encryption (MUST BE CHANGED IN PRODUCTION)
PHI_ENCRYPTION_KEY=your-phi-encryption-key-here-change-this

# OAuth (Optional - configure when needed)
AUTH0_DOMAIN=
AUTH0_CLIENT_ID=
AUTH0_CLIENT_SECRET=
AUTH0_AUDIENCE=

# Wearables (Optional - configure when needed)
FITBIT_CLIENT_ID=
FITBIT_CLIENT_SECRET=
FITBIT_REDIRECT_URI=

# AWS (Optional)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=
AWS_KMS_KEY_ID=

# Email (Optional)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
EMAIL_FROM=noreply@vitanexus.com

# Rate Limiting
RATE_LIMIT_PER_MINUTE=1000

# Feature Flags
ENABLE_WEARABLE_SYNC=true
ENABLE_REAL_TIME_SCORING=true

# Financial Model
COMPANY_PROFIT_SHARE=0.70
MEMBER_REBATE_SHARE=0.30
MINIMUM_INTERVENTION_ROI=1.50

# Compliance
HIPAA_AUDIT_LOG_ENABLED=true
BREACH_NOTIFICATION_EMAIL=security@vitanexus.com

# API Documentation
ENABLE_API_DOCS=true
