# VitaNexus Environment Configuration

# Application
APP_NAME=VitaNexus
APP_VERSION=1.0.0
ENVIRONMENT=development
DEBUG=True
LOG_LEVEL=INFO

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_PREFIX=/v1
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Database
DATABASE_URL=postgresql://vitanexus:password@localhost:5432/vitanexus_dev
DATABASE_POOL_SIZE=20
DATABASE_MAX_OVERFLOW=10

# Redis
REDIS_URL=redis://localhost:6379/0

# Kafka
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_HEALTH_SCORES_TOPIC=health-scores
KAFKA_WEARABLE_METRICS_TOPIC=wearable-metrics

# Security
SECRET_KEY=your-secret-key-change-this-in-production
JWT_SECRET_KEY=your-jwt-secret-key-change-this
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7
PASSWORD_HASH_ALGORITHM=bcrypt

# Encryption (AES-256)
PHI_ENCRYPTION_KEY=your-phi-encryption-key-32-bytes-long

# OAuth Configuration
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret
AUTH0_AUDIENCE=https://api.vitanexus.com

# Wearable Integrations
FITBIT_CLIENT_ID=your-fitbit-client-id
FITBIT_CLIENT_SECRET=your-fitbit-client-secret
FITBIT_REDIRECT_URI=http://localhost:8000/v1/wearables/callback/fitbit

APPLE_HEALTH_TEAM_ID=your-apple-team-id
APPLE_HEALTH_KEY_ID=your-apple-key-id
APPLE_HEALTH_PRIVATE_KEY_PATH=/path/to/private/key.p8

GARMIN_CONSUMER_KEY=your-garmin-consumer-key
GARMIN_CONSUMER_SECRET=your-garmin-consumer-secret

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_S3_BUCKET=vitanexus-documents
AWS_KMS_KEY_ID=your-kms-key-id

# Email
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USER=apikey
SMTP_PASSWORD=your-sendgrid-api-key
EMAIL_FROM=noreply@vitanexus.com

# Monitoring
DATADOG_API_KEY=your-datadog-api-key
DATADOG_APP_KEY=your-datadog-app-key
DATADOG_ENABLED=false

# Rate Limiting
RATE_LIMIT_PER_MINUTE=1000
RATE_LIMIT_BURST=100

# Feature Flags
ENABLE_WEARABLE_SYNC=true
ENABLE_REAL_TIME_SCORING=true
ENABLE_AUTOMATED_INTERVENTIONS=false

# Financial Model Parameters
COMPANY_PROFIT_SHARE=0.70
MEMBER_REBATE_SHARE=0.30
MINIMUM_INTERVENTION_ROI=1.50
RESERVE_SAFETY_FACTOR=1.35

# Health Scoring Model
HEALTH_SCORING_MODEL_VERSION=1.0.0
COST_PER_RISK_POINT=580

# Compliance
HIPAA_AUDIT_LOG_ENABLED=true
AUDIT_LOG_RETENTION_DAYS=2555
BREACH_NOTIFICATION_EMAIL=security@vitanexus.com

# Development Tools
ENABLE_API_DOCS=true
ENABLE_SWAGGER_UI=true
ENABLE_REDOC=true