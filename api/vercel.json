{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "DEBUG": "false",
    "API_PREFIX": "/v1",
    "ENABLE_API_DOCS": "true"
  }
}
